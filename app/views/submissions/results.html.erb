<%= render 'submissions/result_table', { submissions: submissions_accepted } %>
<%= render 'submissions/result_table', { submissions: submissions_waitlist } %>
<%= render 'submissions/result_table', { submissions: submissions_unaccepted } %>

<aside class="aside-widget aside-widget-fixed aside-widget-dashboard">
  <h2 class="aside-widget-header">Download:</h2>
  <ul class="download-list">
    <li><%= link_to_if !submissions_accepted.empty?, "Accepted", csv_download_accepted_path %></li>
    <li><%= link_to_if !submissions_waitlist.empty?, "Waitlist", csv_download_waitlist_path %></li>
    <li><%= link_to_if !submissions_unaccepted.empty?, "Unaccepted", csv_download_unaccepted_path %></li>
  </ul>
</aside>
